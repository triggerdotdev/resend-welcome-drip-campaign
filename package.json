{
  "name": "@trigger.dev/templates-resend-welcome-drip-campaign",
  "version": "1.0.0",
  "description": "A Trigger.dev template - Send new user events a welcome drip campaign using resend.com and react.email",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsc",
