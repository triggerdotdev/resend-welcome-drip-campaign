    "@react-email/preview": "^0.0.5",
    "@react-email/section": "^0.0.8",
    "@react-email/text": "^0.0.4",
    "@trigger.dev/resend": "^0.1.21",
    "@trigger.dev/sdk": "^0.2.14",
    "react": "^18.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "16",
    "@types/react": "^18.0.28",
    "dotenv": "^16.0.3",
    "react-email": "^1.7.13",
